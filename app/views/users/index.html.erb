<div class="dash-wrap">
  <div class="admin-dash">
    <h1>Users</h1>
    <table class="user-table">
      <tr>
        <th>User</th>
        <th># of Rants</th>
      </tr>
    <% @users.each do |user| %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.rants.length %></td>
        </tr>
    <% end %>
    </table>
  </div>
</div>